{
    "manifest_version": 3,
    "name": "YaBlocker",
    "version": "2.0",
    "description": "Yandex Add Blocker.",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": [
          "*://yandex.ru/*",
          "*://ya.ru/*",
          "*://shedevrum.ai/*",
          "*://dzen.ru/*"
        ],
        "js": ["content_scripts/content.js"]
      }
    ]
}  
